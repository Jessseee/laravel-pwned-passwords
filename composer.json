{
    "name": "nickurt/laravel-pwned-passwords",
    "description": "PwnedPasswords for Laravel 5.8",
    "keywords": ["pwned-passwords", "haveibeenpwned", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "laravel/framework": "5.8.*",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\PwnedPasswords\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\PwnedPasswords\\ServiceProvider"
            ],
            "aliases": {
                "PwnedPasswords": "nickurt\\PwnedPasswords\\Facade"
            }
        }
    }
}